{
  "name": "blue-pencil",
  "version": "1.0.4",
  "description": "Library to censor some given words in a given json",
  "keywords": [
    "censor",
    "censorship",
    "replace",
    "search and replace",
    "blue-pencil",
    "words",
    "regular expresion",
    "regex",
    "confidenciality"
  ],
  "author": "Angel Muelas Martinez",
  "contributors": [
    {
      "name": "Angel Muelas Martinez",
      "email": "angel.muelas@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myolnir/blue-pencil.git"
  },
  "dependencies": {
    "flat": "^4.1.0",
    "lodash": "^4.17.14"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-plugin-jest": "^22.7.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-html-reporter": "^2.5.0",
    "jsdoc-to-markdown": "^5.0.0",
    "rimraf": "^2.6.3"
  },
  "scripts": {
    "test": "jest -c ./tests/unit.config.json --coverage",
